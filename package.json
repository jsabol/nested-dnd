{
	"name": "nested-dnd",
	"license": "MIT",
	"version": "0.1.0",
	"engines": {
		"node": "10.16.2",
		"yarn": "1.15.2"
	},
	"proxy": "http://localhost:3001/",
	"private": true,
	"scripts": {
		"_start-js": "react-scripts start --dir client --prefix ../../node_modules",
		"_watch-css": "node-sass-chokidar client/src/ -o client/src/ --watch --recursive",
		"build": "yarn build-css && react-scripts build --dir client",
		"build-css": "npx node-sass client/src/ -o client/src/",
		"dev-client": "set HTTPS=true && concurrently --kill-others-on-fail \"yarn _watch-css\" \"yarn _start-js\"",
		"dev-server": "nodemon --inspect server/server.js",
		"dev-db": "mongod --dbpath ./server/data",
		"dev-test": "cross-env NODE_ENV=test mocha --exit --require ./.testSetup.js \"server/**/*.test.js\" \"client/**/*.test.js\"",
		"build-icon-options": "cd client/public/icons && dir /s/n/b *.svg > icons.txt && node gi_convert.js > ../../src/assets/icons.js",
		"dev-test-debug": "yarn dev-test --inspect-brk",
		"eject": "react-scripts eject --dir client",
		"heroku-postbuild": "yarn build",
		"report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
		"start": "yarn babel && node server/server.js",
		"test": "yarn build-css && cross-env NODE_ENV=test nyc mocha --exit --timeout=3000 \"server/**/*.test.js\" \"client/**/*.test.js\"",
		"babel": "rm -rf .tmp && babel server server/config server/app/** -d .tmp"
	},
	"dependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.2.2",
		"@babel/preset-env": "^7.2.3",
		"@babel/register": "^7.0.0",
		"animate.css": "^3.5.2",
		"async": "^2.6.0",
		"babel-plugin-istanbul": "^5.1.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
		"babel-polyfill": "^6.26.0",
		"bcrypt-nodejs": "^0.0.3",
		"body-parser": "^1.18.2",
		"bootstrap": "^4",
		"color-js": "^1.0.4",
		"connect-flash": "^0.1.1",
		"connect-mongo": "^2.0.1",
		"connected-react-router": "^6.2.2",
		"cookie-parser": "^1.4.3",
		"cross-env": "^5.1.4",
		"debounce": "^1.1.0",
		"dice-expression-evaluator": "^0.1.2",
		"express": "^4.16.2",
		"express-session": "^1.15.6",
		"express-sslify": "^1.2.0",
		"fantasy-names": "https://github.com/cattegy/fantasy-names.git",
		"game-icons-font": "^0.1.2",
		"history": "^4.7.2",
		"jquery": "^3.3.1",
		"jsdom": "^13.2.0",
		"material-colors": "^1.2.5",
		"mongoose": "^5.2.10",
		"morgan": "^1.9.0",
		"natives": "^1.1.6",
		"node-sass-chokidar": "^1.1.0",
		"passport": "^0.4.0",
		"passport-local": "^1.0.0",
		"popper.js": "^1.14.1",
		"prop-types": "^15.6.0",
		"react": "^16.7.0",
		"react-color": "^2.14.0",
		"react-document-title": "^2.0.3",
		"react-dom": "^16.7.0",
		"react-inlinesvg": "^0.8.1",
		"react-lottie": "^1.2.3",
		"react-redux": "^6.0.0",
		"react-router": "^4.3.1",
		"react-router-dom": "^4.3.1",
		"react-router-redux": "^4.0.8",
		"react-scripts": "git://github.com/cattegy/create-react-app#react-scripts",
		"react-sortablejs": "^1.3.6",
		"react-window": "^1.1.2",
		"redux": "^4.0.1",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"sass-material-colors": "^0.0.5",
		"sifter": "^0.5.3",
		"slugify": "^1.3.4",
		"sortablejs": "^1.8.1",
		"webfontloader": "^1.6.28"
	},
	"devDependencies": {
		"canvas-mock": "^0.0.0",
		"chai": "^4.1.2",
		"chai-as-promised": "^7.1.1",
		"concurrently": "^4.1.0",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"es6-promise": "^4.2.4",
		"eslint": "5.6.0",
		"eslint-plugin-flowtype": "^3.2.1",
		"eslint-plugin-import": "2.14.0",
		"eslint-plugin-jsx-a11y": "6.1.2",
		"eslint-plugin-node": "^8.0.0",
		"ignore-styles": "^5.0.1",
		"isomorphic-fetch": "^2.2.1",
		"mocha": "^5.0.4",
		"nodemon": "^1.17.1",
		"nyc": "^13.1.0",
		"redux-devtools": "^3.5.0",
		"sinon": "^6.3.5",
		"sinon-mongoose": "^2.1.1",
		"stylelint": "^9.10.0",
		"stylelint-config-recommended-scss": "^3.2.0",
		"stylelint-scss": "^3.5.1",
		"supertest": "^3.0.0",
		"why-did-you-update": "^1.0.6"
	},
	"browserslist": {
		"development": [
			"last 2 chrome versions",
			"last 2 firefox versions",
			"last 2 edge versions"
		],
		"production": [
			">1%",
			"last 4 versions",
			"Firefox ESR",
			"not ie < 11"
		]
	},
	"nodemonConfig": {
		"ignore": [
			"client/*",
			"server/data/*"
		]
	},
	"nyc": {
		"all": true,
		"cache": true,
		"require": [
			"@babel/register",
			"./.testSetup.js"
		],
		"reporter": [
			"lcov",
			"text"
		],
		"sourceMap": false,
		"instrument": false
	},
	"stylelint": {
		"extends": [
			"stylelint-config-recommended-scss"
		]
	}
}
