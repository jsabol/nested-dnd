{
  "name": "nested-dnd",
  "license": "MIT",
  "version": "0.1.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "proxy": "http://localhost:3001/",
  "scripts": {
    "_start-js": "react-scripts start --dir client --prefix ../../node_modules",
    "_watch-css": "node-sass-chokidar client/src/ -o client/src/ --watch --recursive",
    "build": "yarn build-css && react-scripts build --dir client",
    "build-css": "node-sass-chokidar client/src/ -o client/src/",
    "dev-client": "concurrently --kill-others-on-fail \"yarn _watch-css\" \"yarn _start-js\"",
    "dev-server": "concurrently --kill-others-on-fail \"mongod --dbpath ./server/data\" \"nodemon --inspect server/server.js\"",
    "dev-test": "cls && cross-env NODE_ENV=test mocha --exit --require ./.testSetup.js 'server/**/*.test.js' 'client/**/*.test.js'",
    "dev-test-debug": "yarn dev-test --inspect-brk",
    "eject": "react-scripts eject --dir client",
    "heroku-postbuild": "yarn build",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "start": "node server/server.js",
    "test": "yarn build-css && cross-env NODE_ENV=test nyc mocha --exit --timeout=3000 \"server/**/*.test.js\" \"client/**/*.test.js\""
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "async": "^2.6.0",
    "babel-preset-env": "^1.6.1",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4",
    "color-js": "^1.0.4",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.4",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "font-awesome": "^4.7.0",
    "game-icons-font": "^0.1.2",
    "jquery": "^3.3.1",
    "mongoose": "^5.0.9",
    "morgan": "^1.9.0",
    "node-sass-chokidar": "^1.1.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "popper.js": "^1.14.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.1.2",
    "react-scripts": "https://github.com/cattegy/create-react-app#react-scripts",
    "react-select": "^1.0.0-rc.5",
    "react-transition-group": "^2.2.1",
    "react-virtualized-select": "^3.1.0",
    "webfontloader": "^1.6.28",
    "why-is-node-running": "^2.0.2"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^3.5.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "es6-promise": "^4.2.4",
    "eslint": "^4.19.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "2.34.1",
    "eslint-plugin-import": "2.6.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-react": "^7.7.0",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^5.0.4",
    "nodemon": "^1.17.1",
    "nyc": "^11.6.0",
    "sinon": "^4.4.6",
    "sinon-mongoose": "^2.1.1",
    "supertest": "^3.0.0"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "server/data/*"
    ]
  },
  "nyc": {
    "all": true,
    "cache": true,
    "require": [
      "babel-register",
      "./.testSetup.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
