{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "react-app",
	"globals": {
		"process": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"modules": true,
			"experimentalObjectRestSpread": true
		}
	},
	"rules": {
		"no-process-env": 0
	}
}