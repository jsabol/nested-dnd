{
  "extends": "plugin:node/recommended",
  "plugins": ["node"],
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "ecmaFeatures": {
    "blockBindings": true
  },
  "globals": {
    "process": true
  },
  "rules": {
    "node/no-unpublished-require": "off"
  }
}
